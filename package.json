{
  "name": "caddy_cli",
  "version": "1.0.0",
  "description": "CLI tool for managing Caddy web server with Docker integration",
  "type": "module",
  "bin": {
    "caddy_cli": "./dist/main.js"
  },
  "scripts": {
    "start": "bun run src/main.ts",
    "dev": "bun --watch run src/main.ts",
    "build": "bun build --target=bun --outdir=dist src/main.ts"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "figlet": "^1.10.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.0.0"
  },
  "author": "dbelyaeff",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dbelyaeff/caddy_cli.git"
  }
}
